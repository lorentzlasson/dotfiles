{
  "mcpServers": {
    "chrome-devtools": {
      "type": "stdio",
      "command": "deno",
      "args": [
        "run",
        "--allow-env",
        "--allow-net",
        "--allow-sys",
        "--allow-read",
        "--allow-write",
        "npm:chrome-devtools-mcp@latest",
        "--browserUrl",
        "http://127.0.0.1:9222"
      ],
      "env": {}
    }
  }
}
