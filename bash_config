export PS1="\t \W \$ "
export VISUAL=nvim
export EDITOR="$VISUAL"

alias obp="nvim ~/.bash_profile"
alias ovrc="nvim ~/.vimrc"
alias rbp="source ~/.bash_profile"
alias cl="clear"
#alias copy="tr -d '\n' | pbcopy" #osx
alias copy="tr -d '\n' | xclip -sel clip" #linux
alias vim="nvim"
alias clg="clear && git status"
