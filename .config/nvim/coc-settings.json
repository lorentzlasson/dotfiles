{
  "languageserver": {
    "elmLS": {
      "command": "elm-language-server",
      "filetypes": ["elm"],
      "rootPatterns": ["elm.json"]
    }
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "json",
    "typscriptreact",
    "elm",
    "javascript",
    "typescript"
  ],
  "typescript.suggest.autoImports": true,
  "javascript.suggestionActions.enabled": false,
  "eslint.autoFixOnSave": true
}
